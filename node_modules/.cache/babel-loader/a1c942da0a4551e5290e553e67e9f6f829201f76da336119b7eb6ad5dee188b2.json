{"ast":null,"code":"import router from \"@/router\";\nimport { SimpleBar } from \"simplebar-vue3\";\nimport { layoutComputed } from \"@/state/helpers\";\nimport NavBar from \"@/components/nav-bar\";\nimport Menu from \"@/components/menu.vue\";\nimport RightBar from \"@/components/right-bar\";\nimport Footer from \"@/components/footer\";\nlocalStorage.setItem('hoverd', false);\n\n/**\n * Vertical layout\n */\nexport default {\n  components: {\n    NavBar,\n    RightBar,\n    Footer,\n    SimpleBar,\n    Menu\n  },\n  data() {\n    return {\n      isMenuCondensed: false\n    };\n  },\n  computed: {\n    ...layoutComputed,\n    sidebarSize: {\n      get() {\n        return this.$store ? this.$store.state.layout.sidebarSize : {} || {};\n      },\n      set(type) {\n        return this.changeSidebarSize({\n          sidebarSize: type\n        });\n      }\n    }\n  },\n  created: () => {\n    document.body.removeAttribute(\"data-layout\", \"horizontal\");\n    document.body.removeAttribute(\"data-topbar\", \"dark\");\n    document.body.removeAttribute(\"data-layout-size\", \"boxed\");\n  },\n  methods: {\n    initActiveMenu() {\n      if (document.documentElement.getAttribute('data-sidebar-size') === 'sm-hover') {\n        localStorage.setItem('hoverd', true);\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover-active');\n      } else if (document.documentElement.getAttribute('data-sidebar-size') === 'sm-hover-active') {\n        localStorage.setItem('hoverd', false);\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover');\n      } else {\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover');\n      }\n    },\n    toggleMenu() {\n      document.body.classList.toggle(\"sidebar-enable\");\n      if (window.screen.width >= 992) {\n        // eslint-disable-next-line no-unused-vars\n        router.afterEach((routeTo, routeFrom) => {\n          document.body.classList.remove(\"sidebar-enable\");\n          document.body.classList.remove(\"vertical-collpsed\");\n        });\n        document.body.classList.toggle(\"vertical-collpsed\");\n      } else {\n        // eslint-disable-next-line no-unused-vars\n        router.afterEach((routeTo, routeFrom) => {\n          document.body.classList.remove(\"sidebar-enable\");\n        });\n        document.body.classList.remove(\"vertical-collpsed\");\n      }\n      this.isMenuCondensed = !this.isMenuCondensed;\n    },\n    toggleRightSidebar() {\n      document.body.classList.toggle(\"right-bar-enabled\");\n    },\n    hideRightSidebar() {\n      document.body.classList.remove(\"right-bar-enabled\");\n    }\n  },\n  mounted() {\n    if (localStorage.getItem('hoverd') == 'true') {\n      document.documentElement.setAttribute('data-sidebar-size', 'sm-hover-active');\n    }\n    document.getElementById('overlay').addEventListener('click', () => {\n      document.body.classList.remove('vertical-sidebar-enable');\n    });\n  }\n};","map":{"version":3,"names":["router","SimpleBar","layoutComputed","NavBar","Menu","RightBar","Footer","localStorage","setItem","components","data","isMenuCondensed","computed","sidebarSize","get","$store","state","layout","set","type","changeSidebarSize","created","document","body","removeAttribute","methods","initActiveMenu","documentElement","getAttribute","setAttribute","toggleMenu","classList","toggle","window","screen","width","afterEach","routeTo","routeFrom","remove","toggleRightSidebar","hideRightSidebar","mounted","getItem","getElementById","addEventListener"],"sources":["C:\\Users\\Sekou A Sidibe\\Documents\\NTA TECH\\projets\\klispay_backoffice\\v3\\src\\layouts\\vertical.vue"],"sourcesContent":["<script>\nimport router from \"@/router\";\nimport { SimpleBar } from \"simplebar-vue3\";\nimport { layoutComputed } from \"@/state/helpers\";\n\nimport NavBar from \"@/components/nav-bar\";\nimport Menu from \"@/components/menu.vue\";\nimport RightBar from \"@/components/right-bar\";\nimport Footer from \"@/components/footer\";\nlocalStorage.setItem('hoverd', false);\n\n/**\n * Vertical layout\n */\nexport default {\n  components: { NavBar, RightBar, Footer, SimpleBar, Menu },\n  data() {\n    return {\n      isMenuCondensed: false,\n    };\n  },\n  computed: {\n    ...layoutComputed,\n    sidebarSize: {\n      get() {\n        return this.$store ? this.$store.state.layout.sidebarSize : {} || {};\n      },\n      set(type) {\n        return this.changeSidebarSize({\n          sidebarSize: type,\n        });\n      },\n    },\n  },\n  created: () => {\n    document.body.removeAttribute(\"data-layout\", \"horizontal\");\n    document.body.removeAttribute(\"data-topbar\", \"dark\");\n    document.body.removeAttribute(\"data-layout-size\", \"boxed\");\n  },\n  methods: {\n    initActiveMenu() {\n      if (document.documentElement.getAttribute('data-sidebar-size') === 'sm-hover') {\n        localStorage.setItem('hoverd', true);\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover-active');\n      } else if (document.documentElement.getAttribute('data-sidebar-size') === 'sm-hover-active') {\n        localStorage.setItem('hoverd', false);\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover');\n      } else {\n        document.documentElement.setAttribute('data-sidebar-size', 'sm-hover');\n      }\n    },\n    toggleMenu() {\n      document.body.classList.toggle(\"sidebar-enable\");\n\n      if (window.screen.width >= 992) {\n        // eslint-disable-next-line no-unused-vars\n        router.afterEach((routeTo, routeFrom) => {\n          document.body.classList.remove(\"sidebar-enable\");\n          document.body.classList.remove(\"vertical-collpsed\");\n        });\n        document.body.classList.toggle(\"vertical-collpsed\");\n      } else {\n        // eslint-disable-next-line no-unused-vars\n        router.afterEach((routeTo, routeFrom) => {\n          document.body.classList.remove(\"sidebar-enable\");\n        });\n        document.body.classList.remove(\"vertical-collpsed\");\n      }\n      this.isMenuCondensed = !this.isMenuCondensed;\n    },\n    toggleRightSidebar() {\n      document.body.classList.toggle(\"right-bar-enabled\");\n    },\n    hideRightSidebar() {\n      document.body.classList.remove(\"right-bar-enabled\");\n    },\n\n  },\n  mounted() {\n    if (localStorage.getItem('hoverd') == 'true') {\n      document.documentElement.setAttribute('data-sidebar-size', 'sm-hover-active');\n    }\n\n    document.getElementById('overlay').addEventListener('click', () => {\n      document.body.classList.remove('vertical-sidebar-enable');\n    });\n  }\n};\n</script>\n  \n<template>\n  <div id=\"layout-wrapper\">\n    <NavBar />\n    <div>\n      <!-- ========== Left Sidebar Start ========== -->\n      <!-- ========== App Menu ========== -->\n      <div class=\"app-menu navbar-menu\">\n        <!-- LOGO -->\n        <div class=\"navbar-brand-box\">\n          <!-- Dark Logo-->\n          <router-link to=\"/\" class=\"logo logo-dark\">\n            <span class=\"logo-sm\">\n              <img src=\"@/assets/images/logo-sm.png\" alt=\"\" height=\"22\" />\n            </span>\n            <span class=\"logo-lg\">\n              <img src=\"@/assets/images/logo-dark.png\" alt=\"\" height=\"17\" />\n            </span>\n          </router-link>\n          <!-- Light Logo-->\n          <router-link to=\"/\" class=\"logo logo-light\">\n            <span class=\"logo-sm\">\n              <img src=\"@/assets/images/logo-sm.png\" alt=\"\" height=\"22\" />\n            </span>\n            <span class=\"logo-lg\">\n              <img src=\"@/assets/images/logo-light.png\" alt=\"\" height=\"17\" />\n            </span>\n          </router-link>\n          <button type=\"button\" class=\"btn btn-sm p-0 fs-20 header-item float-end btn-vertical-sm-hover\"\n            id=\"vertical-hover\" @click=\"initActiveMenu\">\n            <i class=\"ri-record-circle-line\"></i>\n          </button>\n        </div>\n\n        <SimpleBar id=\"scrollbar\" class=\"h-100\" ref=\"scrollbar\">\n          <Menu></Menu>\n        </SimpleBar>\n        <div class=\"sidebar-background\"></div>\n      </div>\n      <!-- Left Sidebar End -->\n      <!-- Vertical Overlay-->\n      <div class=\"vertical-overlay\" id=\"overlay\"></div>\n    </div>\n    <!-- ============================================================== -->\n    <!-- Start Page Content here -->\n    <!-- ============================================================== -->\n\n    <div class=\"main-content\">\n      <div class=\"page-content\">\n        <!-- Start Content-->\n        <b-container fluid>\n          <slot />\n        </b-container>\n      </div>\n      <Footer />\n    </div>\n    <RightBar />\n  </div>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,UAAU;AAC7B,SAASC,SAAQ,QAAS,gBAAgB;AAC1C,SAASC,cAAa,QAAS,iBAAiB;AAEhD,OAAOC,MAAK,MAAO,sBAAsB;AACzC,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,QAAO,MAAO,wBAAwB;AAC7C,OAAOC,MAAK,MAAO,qBAAqB;AACxCC,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAE,KAAK,CAAC;;AAErC;;;AAGA,eAAe;EACbC,UAAU,EAAE;IAAEN,MAAM;IAAEE,QAAQ;IAAEC,MAAM;IAAEL,SAAS;IAAEG;EAAK,CAAC;EACzDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGV,cAAc;IACjBW,WAAW,EAAE;MACXC,GAAGA,CAAA,EAAG;QACJ,OAAO,IAAI,CAACC,MAAK,GAAI,IAAI,CAACA,MAAM,CAACC,KAAK,CAACC,MAAM,CAACJ,WAAU,GAAI,CAAC,KAAK,CAAC,CAAC;MACtE,CAAC;MACDK,GAAGA,CAACC,IAAI,EAAE;QACR,OAAO,IAAI,CAACC,iBAAiB,CAAC;UAC5BP,WAAW,EAAEM;QACf,CAAC,CAAC;MACJ;IACF;EACF,CAAC;EACDE,OAAO,EAAEA,CAAA,KAAM;IACbC,QAAQ,CAACC,IAAI,CAACC,eAAe,CAAC,aAAa,EAAE,YAAY,CAAC;IAC1DF,QAAQ,CAACC,IAAI,CAACC,eAAe,CAAC,aAAa,EAAE,MAAM,CAAC;IACpDF,QAAQ,CAACC,IAAI,CAACC,eAAe,CAAC,kBAAkB,EAAE,OAAO,CAAC;EAC5D,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAIJ,QAAQ,CAACK,eAAe,CAACC,YAAY,CAAC,mBAAmB,MAAM,UAAU,EAAE;QAC7ErB,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAE,IAAI,CAAC;QACpCc,QAAQ,CAACK,eAAe,CAACE,YAAY,CAAC,mBAAmB,EAAE,iBAAiB,CAAC;MAC/E,OAAO,IAAIP,QAAQ,CAACK,eAAe,CAACC,